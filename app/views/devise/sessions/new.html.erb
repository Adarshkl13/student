<div class="container">
<div class="card">
<div class="col-md-6 col-md-offset-3">
<h2 class="text-center sign-head"><b>Log in</b></h2>


<%= form_for(resource, as: resource_name, url: session_path(resource_name),html: { data: { turbo: false} }) do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email",:maxlength=>"80",class: "form-control" %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "current-password",:maxlength=>"80",class: "form-control" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>
</br>
  <div class="actions">
    <%= f.submit "Log in",class: "btn btn-dark" %>
  </div>
<% end %>
</br>
<%= render "devise/shared/links" %>

</div>
</div>